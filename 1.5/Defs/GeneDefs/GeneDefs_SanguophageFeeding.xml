<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<GeneDef>
		<defName>VRE_AnimalFeeder</defName>
		<label>animalfeeder</label>
		<description>Carriers of this gene have small retractable fangs and an organ on the roof of the mouth which can extract hemogen from fresh warm blood. They can bite any animal, suck the blood, and gain hemogen directly. This can sometimes make the animal attack the gene carrier.</description>
		<iconPath>UI/Icons/Genes/VRE_AnimalFeeder</iconPath>
		<geneClass>Gene_Bloodfeeder</geneClass>
		<prerequisite>Hemogenic</prerequisite>
		<displayCategory>Hemogen</displayCategory>
		<abilities>
			<li>VRE_AnimalFeed</li>
		</abilities>
		<descriptionHyperlinks>
			<AbilityDef>VRE_AnimalFeed</AbilityDef>
		</descriptionHyperlinks>
		<biostatMet>-1</biostatMet>
		<biostatCpx>1</biostatCpx>
		<minAgeActive>3</minAgeActive>
		<symbolPack>
			<wholeNameSymbols>
				<li>
					<symbol>animalfeeder</symbol>
				</li>
			</wholeNameSymbols>
			<prefixSymbols>
				<li>
					<symbol>hemo</symbol>
				</li>
				<li>
					<symbol>blood</symbol>
				</li>
				<li>
					<symbol>vamp</symbol>
				</li>
				<li>
					<symbol>fang</symbol>
				</li>
			</prefixSymbols>
			<suffixSymbols>
				<li>
					<symbol>fang</symbol>
				</li>
				<li>
					<symbol>eater</symbol>
				</li>
				<li>
					<symbol>ubus</symbol>
				</li>
			</suffixSymbols>
		</symbolPack>
	</GeneDef>

